<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane fx:id="loginPane" prefHeight="400.0" prefWidth="300.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LoginController">
   <children>
      <Button fx:id="loginButton" layoutX="100.0" layoutY="280.0" mnemonicParsing="false" onAction="#loginComplete" prefHeight="25.0" prefWidth="88.0" text="Login" />
      <TextField fx:id="userUsername" layoutX="25.0" layoutY="106.0" prefHeight="25.0" prefWidth="251.0" promptText="Enter Username" />
      <PasswordField fx:id="userPassword" layoutX="26.0" layoutY="162.0" prefHeight="25.0" prefWidth="251.0" promptText="Enter Password" />
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="50.0" layoutY="38.0" prefHeight="17.0" prefWidth="200.0" text="SAN JUAN LIBRARY">
         <font>
            <Font name="Century Gothic Bold Italic" size="18.0" />
         </font>
      </Label>
      <Label layoutX="26.0" layoutY="89.0" prefHeight="17.0" prefWidth="65.0" text="Username">
         <font>
            <Font size="10.0" />
         </font>
      </Label>
      <Label layoutX="26.0" layoutY="145.0" prefHeight="17.0" prefWidth="65.0" text="Password">
         <font>
            <Font size="10.0" />
         </font>
      </Label>
      <Label fx:id="signUp" alignment="CENTER" layoutX="100.0" layoutY="219.0" onMouseClicked="#signUp" prefHeight="17.0" prefWidth="88.0" text="Sign Up" textFill="#0026ff" underline="true">
         <font>
            <Font size="10.0" />
         </font>
      </Label>
      <Label fx:id="employee" alignment="CENTER" layoutX="100.0" layoutY="249.0" onMouseClicked="#employee" prefHeight="17.0" prefWidth="88.0" text="Employee" textFill="#0026ff" underline="true">
         <font>
            <Font size="10.0" />
         </font>
      </Label>
   </children>
</Pane>
