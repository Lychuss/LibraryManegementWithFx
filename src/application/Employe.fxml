<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane fx:id="loginPane" prefHeight="400.0" prefWidth="300.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LoginEmployeeController">
   <children>
      <Button fx:id="loginEmployeeButton" layoutX="52.0" layoutY="243.0" mnemonicParsing="false" onAction="#loginEmployee" text="Login Account" />
      <TextField fx:id="loginEmployeeUsername" layoutX="23.0" layoutY="106.0" prefHeight="25.0" prefWidth="251.0" promptText="Enter Username" />
      <PasswordField fx:id="loginEmployeePassword" layoutX="23.0" layoutY="162.0" prefHeight="25.0" prefWidth="251.0" promptText="Enter Password" />
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="34.0" layoutY="46.0" prefHeight="17.0" prefWidth="229.0" text="EMPLOYEE">
         <font>
            <Font name="Century Gothic Bold Italic" size="18.0" />
         </font>
      </Label>
      <Label layoutX="26.0" layoutY="89.0" prefHeight="17.0" prefWidth="88.0" text="Username">
         <font>
            <Font size="10.0" />
         </font>
      </Label>
      <Label layoutX="26.0" layoutY="145.0" prefHeight="17.0" prefWidth="88.0" text="Password">
         <font>
            <Font size="10.0" />
         </font>
      </Label>
      <Button fx:id="loginCancelButton" layoutX="171.0" layoutY="243.0" mnemonicParsing="false" onAction="#cancelButton" text="Cancel" />
   </children>
</Pane>
